{
  "name": "roadey",
  "version": "1.0.0",
  "description": "Roadey - Express server",
  "main": "src/index.ts",
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "author": "Reba Pipes",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/pubsub": "^2.2.0",
    "@google-cloud/storage": "^5.1.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "pg": "^8.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/jest": "^26.0.4",
    "@types/pg": "^7.14.4",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
